#!/bin/sh
exec 2>&1
source <%= node['graphite']['base_dir'] %>/.venv/bin/activate
exec chpst -u <%= node['graphite']['carbon']['user'] %>:<%= node['graphite']['carbon']['group'] %> -l <%= node['graphite']['base_dir'] %>/storage/carbon-cache.lock -- <%= node['graphite']['base_dir']%>/bin/carbon-cache.py --debug start
